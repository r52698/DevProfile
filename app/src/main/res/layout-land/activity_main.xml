<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent">

    <ImageView
            android:layout_width="0dp"
            android:layout_height="120dp" app:srcCompat="@drawable/devslopesbanner"
            android:id="@+id/imageView17"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:scaleType="centerCrop"/>
    <ImageView
            android:layout_width="100dp"
            android:layout_height="wrap_content" app:srcCompat="@drawable/devslopesprofilelogo"
            android:id="@+id/logo" android:layout_marginTop="100dp"
            app:layout_constraintTop_toTopOf="@+id/imageView17" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp"/>
    <TextView
            android:text="Devslopes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3" android:textSize="24sp"
            app:layout_constraintTop_toBottomOf="@+id/imageView17" app:layout_constraintStart_toEndOf="@+id/logo"
            android:layout_marginStart="24dp" android:layout_marginTop="24dp"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/camerabutton"
            android:id="@+id/imageView26"
            app:layout_constraintEnd_toStartOf="@+id/imageView27" android:layout_marginEnd="24dp"
            app:layout_constraintTop_toTopOf="@+id/imageView27"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/favoritebutton"
            android:id="@+id/imageView27"
            app:layout_constraintEnd_toStartOf="@+id/imageView28" android:layout_marginEnd="24dp"
            app:layout_constraintTop_toTopOf="@+id/imageView28"/>
    <ImageView
            android:layout_width="60dp"
            android:layout_height="60dp" app:srcCompat="@drawable/inboxbutton"
            android:id="@+id/imageView28"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="16dp" android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView17" app:layout_constraintBottom_toBottomOf="@+id/logo"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/dataimage"
            android:id="@+id/imageView29"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView30" app:layout_constraintTop_toTopOf="@+id/imageView30"
            app:layout_constraintBottom_toBottomOf="@+id/imageView30" app:layout_constraintDimensionRatio="1:1"
            android:scaleType="centerCrop" android:layout_marginStart="16dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/imagesimage"
            android:id="@+id/imageView30"
            app:layout_constraintStart_toEndOf="@+id/imageView29" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView31" app:layout_constraintTop_toTopOf="@+id/imageView31"
            app:layout_constraintBottom_toBottomOf="@+id/imageView31" app:layout_constraintDimensionRatio="1:1"
            android:scaleType="centerCrop" android:layout_marginStart="8dp" android:layout_marginEnd="8dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/videosimage"
            android:id="@+id/imageView31"
            app:layout_constraintStart_toEndOf="@+id/imageView30" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView32" app:layout_constraintTop_toTopOf="@+id/imageView32"
            app:layout_constraintBottom_toBottomOf="@+id/imageView32" app:layout_constraintDimensionRatio="1:1"
            android:scaleType="centerCrop" android:layout_marginStart="8dp" android:layout_marginEnd="8dp"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/codeimage"
            android:id="@+id/imageView32"
            app:layout_constraintStart_toEndOf="@+id/imageView31" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginBottom="32dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintDimensionRatio="1:1"
            android:scaleType="centerCrop" android:layout_marginStart="8dp" android:layout_marginEnd="16dp"
            android:layout_marginTop="32dp" app:layout_constraintTop_toBottomOf="@+id/logo"/>
</android.support.constraint.ConstraintLayout>